/**
 * Implements hook_views_api().
 */
function stanford_search_api_db_views_api($module = NULL, $api = NULL) {
  return array("api" => "3.0");
}

/**
 * Implements hook_ctools_plugin_api().
 */
function stanford_search_api_db_ctools_plugin_api() {
  list($module, $api) = func_get_args();
  if ($module == "facetapi" && $api == "facetapi_defaults") {
    return array("version" => "1");
  }
}


/**
 * Implements hook_default_search_api_server()
 * @return [type] [description]
 */
function stanford_search_api_db_default_search_api_server() {
  $defaults = array();

  $search_db_nodes = array();
  $search_db_nodes["name"] = t("Search Database Nodes");
  $search_db_nodes["machine_name"] = "search_database_nodes";
  $search_db_nodes["description"] = t("A default search database of all node types using search api's search database module.");
  $search_db_nodes["class"] = "search_api_db_service";
  $search_db_nodes["options"] = array();
  $search_db_nodes["options"]["database"] = "default:default";
  $search_db_nodes["options"]["min_chars"] = 3;
  $search_db_nodes["options"]["partial_matches"] = 0;
  $search_db_nodes["options"]["autocomplete"] = array();
  $search_db_nodes["options"]["autocomplete"]["suggest_suffix"] = 1;
  $search_db_nodes["options"]["autocomplete"]["suggest_words"] = 1;
  $search_db_nodes["options"]["enabled"] = 1;

  $defaults['search_database_nodes'] = entity_create("search_api_server", $search_db_nodes);

  return $defaults;
}

